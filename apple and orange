This was a bit exciting. I have addictions to for loops :P
challenge link-https://www.hackerrank.com/challenges/apple-and-orange/problem

#include<bits/stdc++.h>
using namespace std;
int main()
{ int s,t,a,b,m,n;

 cin>>s>>t>>a>>b>>m>>n;
 
int ar[m];int ar1[n];
 for(int i=0;i<m;i++)
 {
     cin>>ar[i];
 }
 
 for(int i=0;i<n;i++)
 {
     cin>>ar1[i];
 }
  
     int temp1[m];int temp2[n];
 for(int i=0;i<m;i++)
   
 {temp1[i]=a+ar[i];

 }
  for(int i=0;i<n;i++)
     temp2[i]=b+ar1[i];
    
 int c1=0;int c2=0;
 for(int i=0;i<m;i++)
 {
     if(temp1[i]>=s&&temp1[i]<=t)
         c1++;
 } for(int i=0;i<n;i++)
     {if(temp2[i]>=s&&temp2[i]<=t)
          c2++;
     }   
  
 cout<<c1<<"\n"<<c2;
  
    return 0;
}
    
