//challenge https://www.hackerrank.com/challenges/security-tutorial-functions/problem


#include <bits/stdc++.h>

using namespace std;

// Complete the calculate function below.
int calculate(int x) {
int res;
    res=x%11;
    return res;

}

int main()
{
    ofstream fout(getenv("OUTPUT_PATH"));

    int x;
    cin >> x;
    cin.ignore(numeric_limits<streamsize>::max(), '\n');

    int result = calculate(x);

    fout << result << "\n";

    fout.close();

    return 0;
}
