#include<bits/stdc++.h>
using namespace std;
int main()
{
    unsigned int arr[6][6];
    for(int i=0;i<6;i++)
        {
            for(int j=0;j<6;j++)
            {
                cin>>arr[i][j];
            }
        }
    int sum;int temp=0;int r=0;int p=0;
    for(int i=1;i<5;i++)
        {    sum=0; 
            for(int j=1;j<5;j++)
            {
sum+= arr[i][j]+arr[i-1][j-1]+arr[i-1][j]+arr[i-1][j+1]       +arr[i+1][j-1]+arr[i+1][j]+arr[i+1][j+1];
        /*       if(i==1&&j==1)
                   cout<<sum<<" ";
                else
                cout<<sum-temp<<" ";
               temp=sum; 
         */
               if(i==1&&j==1)
                   r=sum;
                else
                r=sum-temp;
                
                
          
          }
         if(r>p)
             p=r;
             
       
        }
    cout<<p;
    
    
    
    return 0;
}
